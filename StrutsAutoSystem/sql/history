*テーブル作成
create table histories(
    id int NOT NULL AUTO_INCREMENT PRIMARY KEY,
    user_id int NOT NULL,
    point int NOT NULL,
    createed_at timestamp NOT NULL
    ) ENGINE=InnoDB DEFAULT CHARSET=utf8;

